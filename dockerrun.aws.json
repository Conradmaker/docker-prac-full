{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "fe",
            "image": "conradmaker/docker-client",
            "hostname": "fe",
            "essential": false
        },
        {
            "name": "be",
            "image": "conradmaker/docker-server",
            "hostname": "be",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "conradmaker/docker-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["fe","be"]
        }
    ]
}